{
    "name": "cnvs/canvas",
    "description": "A Laravel publishing platform",
    "keywords": ["canvas", "laravel", "publishing", "platform"],
    "license": "MIT",
    "homepage": "https://cnvs.io",
    "support": {
        "issues": "https://github.com/cnvs/canvas/issues",
        "source": "https://github.com/cnvs/canvas"
    },
    "authors": [
        {
            "name": "Todd Austin",
            "email": "austin.todd.j@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "doctrine/dbal": "^2.6|^2.7|^2.8|^2.9|^2.10",
        "illuminate/support": "^5.6|^5.7|^5.8|^6.0",
        "spatie/laravel-blade-javascript": "^2.1|^2.2|^2.3|^2.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.6|^3.7|^3.8|^4.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Canvas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Canvas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Canvas\\CanvasServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
